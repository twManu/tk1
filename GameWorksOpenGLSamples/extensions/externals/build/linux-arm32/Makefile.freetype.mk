freetype_debug_hpaths    := 
freetype_debug_hpaths    += ./../../src/freetype-2.4.9/include
freetype_debug_hpaths    += ./../../src/freetype-2.4.9/src
freetype_debug_hpaths    += ./../../include
freetype_debug_lpaths    := 
freetype_debug_defines   := $(freetype_custom_defines)
freetype_debug_defines   += FT2_BUILD_LIBRARY
freetype_debug_defines   += LINUX=1
freetype_debug_defines   += _LIB
freetype_debug_defines   += GLEW_NO_GLU=1
freetype_debug_defines   += _DEBUG
freetype_debug_libraries := 
freetype_debug_libraries += pthread
freetype_debug_libraries += glfw3
freetype_debug_cflags    := $(freetype_custom_cflags)
freetype_debug_cflags    += $(addprefix -D, $(freetype_debug_defines))
freetype_debug_cflags    += $(addprefix -I, $(freetype_debug_hpaths))
freetype_debug_cflags  += -g -fno-omit-frame-pointer -marm -funwind-tables -Wall -Wextra -Wno-unused-parameter -Wno-ignored-qualifiers -Wno-unused-but-set-variable -Wno-switch -Wno-unused-variable -Wno-unused-function -Wno-reorder
freetype_debug_cflags  += -O0 -ggdb
freetype_debug_lflags    := $(freetype_custom_lflags)
freetype_debug_lflags    += $(addprefix -L, $(freetype_debug_lpaths))
freetype_debug_lflags    += -Wl,--start-group $(addprefix -l, $(freetype_debug_libraries)) -Wl,--end-group
freetype_debug_objsdir  := $(OBJS_DIR)/freetype_debug
freetype_debug_obj      := 
freetype_debug_c_o    = $(addprefix $(freetype_debug_objsdir)/, $(subst ./, , $(subst ../, , $(patsubst %.c, %.c.o, $(freetype_cfiles)))))
freetype_debug_obj      += $(freetype_debug_c_o) 
freetype_debug_bin      := ./../../lib/linux-arm32/libfreetypeD.a

clean_freetype_debug: 
	@$(ECHO) clean freetype debug
	@$(RMDIR) $(freetype_debug_objsdir)
	@$(RMDIR) $(freetype_debug_bin)

build_freetype_debug: postbuild_freetype_debug
postbuild_freetype_debug: mainbuild_freetype_debug
mainbuild_freetype_debug: prebuild_freetype_debug $(freetype_debug_bin)
prebuild_freetype_debug:

-include $(freetype_debug_c_o:%o=%d)

$(freetype_debug_bin): $(freetype_debug_obj) 
	@mkdir -p `dirname ./../../lib/linux-arm32/libfreetypeD.a`
	@$(AR) r $(freetype_debug_bin) $^
	@$(RANLIB) $(freetype_debug_bin)
	@$(ECHO) building $@ complete!

$(freetype_debug_c_o): $(freetype_debug_objsdir)/%.o: 
	@$(ECHO) freetype: compiling debug $(filter %$*,$(freetype_cfiles))...
	@mkdir -p $(dir $(@))
	$(CC) $(freetype_debug_cflags) -MMD -MP -MF $(subst .c.o,.c.d,$@) -c $(filter %$*,$(freetype_cfiles)) -o $@
